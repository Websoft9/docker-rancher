{
  "name": "rancher",
  "trademark": "Rancher",
  "installpath": "/data/wwwroot/rancher",
  "compose_file": "docker-compose.yml",
  "fork_url": "",
  "env_file": ".env",
  "requirements":{
    "cpu_arch": "Linux x86-64, ARM 32/64, Windows x86-64, IBM POWER8, x86/i686",
    "cpu": "2",
    "memory": "4",
    "disk": "20",
    "swap": "2",
    "url": "https://github.com/rancher/docker#recommended-system-requirements"
  },

  "user": {
    "username": "admin",
    "password": "123456"
  },

  "services":[
    {
      "name": "rancher-server",
      "port": "9002",
      "description_cn": "浏览器访问 Rancher",
      "description_en": "Web-GUI database management tool",
      "required": "Y"
    },

    {
      "name": "phpmyadmin",
      "port": "9090",
      "description_cn": "数据库可视化管理工具",
      "description_en": "Web-GUI database management tool",
      "required": "Y"
    }
  ]

}
